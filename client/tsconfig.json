{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "outDir": "./dist",
    "rootDir": "./src",
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src"
  ]
}